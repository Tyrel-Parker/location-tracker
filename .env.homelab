# Example .env for HOMELAB with nginx

# Database Configuration
POSTGRES_DB=location_tracker
POSTGRES_USER=tracker_user
POSTGRES_PASSWORD=your_secure_password_here

# Port Configuration
POSTGRES_PORT=5432
API_PORT=5000

# Domain Configuration (same for both locations)
DOMAIN=location-api.yourdomain.com

# Environment identifier
ENVIRONMENT=homelab

# Reverse Proxy Type
PROXY_TYPE=nginx
