# Database Configuration
POSTGRES_DB=location_tracker
POSTGRES_USER=tracker_user
POSTGRES_PASSWORD=change_this_password

# Port Configuration
POSTGRES_PORT=5432
API_PORT=5000

# Domain Configuration
# Use your Cloudflare tunnel domain (same for both locations)
DOMAIN=location-api.yourdomain.com

# Environment identifier (for logging/debugging)
ENVIRONMENT=homelab

# Reverse Proxy Type: traefik, nginx, or none
PROXY_TYPE=traefik
